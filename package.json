{
  "name": "blink1control2",
  "version": "0.1.0",
  "description": "Blink1Control implemented in ElectronJs, NodeJs, ReactJs",
  "main": "src/main.js",
  "homepage": "http://github.com/todbot/blink1control2",
  "bugs": {
    "url": "http://github.com/todbot/blink1control2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:todbot/blink1control2.git"
  },
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
    "start-win": "set NODE_ENV=development && node_modules\\electron-prebuilt\\dist\\electron.exe .",
    "startp": "NODE_ENV=production ./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
    "env": "env",
    "watch": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --hot --inline",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "build-osx": "NODE_ENV=production webpack -p --config webpack.config.js; ./node_modules/.bin/electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --prune --asar --platform=darwin --arch=x64 --version=$npm_package_electronVersion --icon=assets/epp.icns",
    "build-win": "NODE_ENV=production webpack -p --config webpack.config.js; ./node_modules/.bin/electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --prune --asar --platform=win32 --arch=all --version=$npm_package_electronVersion --icon=assets/epp.ico",
    "build-win2": "set NODE_ENV=production && webpack --config webpack.config.js && node_modules\\.bin\\electron-packager . Blink1Control2 --overwrite --out=dist --ignore='^/dist$' --prune --asar --platform=win32 --arch=x64 --version=0.36.5 --icon=assets/epp.ico",
    "clean": "rm -rf node_modules",
    "preinstall": "electron-rebuild -v 0.36.5 -p -f -m . -w node-hid -e node_modules/electron-prebuilt"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-rebuild": "^1.1.3",
    "font-awesome": "^4.5.0",
    "imap": "^0.8.16",
    "lodash": "^4.2.1",
    "moment": "^2.11.2",
    "nconf": "^0.8.4",
    "node-blink1": "^0.2.2",
    "node-hid": "^0.5.1",
    "object-assign": "^4.0.1",
    "react": "^0.14.7",
    "react-addons-linked-state-mixin": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-color": "^1.3.6",
    "react-fontawesome": "^0.3.3",
    "request": "^2.69.0",
    "simplecrypt": "^0.1.0",
    "tinycolor2": "^1.3.0",
    "usb-detection": "^1.3.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-react": "^6.3.13",
    "bootstrap": "^3.3.6",
    "css-loader": "^0.23.1",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.5",
    "electron-rebuild": "^1.1.3",
    "file-loader": "^0.8.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "node-libs-browser": "^1.0.0",
    "react-dom": "^0.14.7",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
