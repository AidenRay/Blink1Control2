{
  "name": "blink1control2",
  "productName": "Blink1Control",
  "author": "ThingM",
  "companyName": "ThingM",
  "version": "0.1.0",
  "description": "Blink1Control implemented in ElectronJs, NodeJs, ReactJs",
  "main": "src/main.js",
  "homepage": "http://github.com/todbot/blink1control2",
  "bugs": {
    "url": "http://github.com/todbot/blink1control2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:todbot/blink1control2.git"
  },
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
    "startp": "./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
    "start-win": "set NODE_ENV=development&& node_modules\\electron-prebuilt\\dist\\electron.exe .",
    "startp-win": "node_modules\\electron-prebuilt\\dist\\electron.exe .",
    "env": "env",
    "watch": "NODE_ENV=development webpack-dev-server --hot --inline",
    "pack": "webpack -p",
    "build-osx": "webpack -p --config webpack.config.js && electron-packager . $npm_package_productName --overwrite --out=dist --prune --asar --platform=darwin --arch=x64 --version=$npm_package_electronVersion --icon=assets/blink1-icon0.icns --ignore='^/dist$'",
    "build-win": "webpack -p --config webpack.config.js && electron-packager . $npm_package_productName --overwrite --out=dist --prune --asar --platform=win32 --arch=x64 --version=$npm_package_electronVersion --icon=assets/epp.icns --ignore='^/dist$'",
    "build-win1": "webpack -p --config webpack.config.js; node_modules\\.bin\\electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --prune --asar --platform=win32 --arch=all --version=$npm_package_electronVersion --icon=assets/epp.ico",
    "build-win2": "webpack --config webpack.config.js && node_modules\\.bin\\electron-packager . Blink1Control2 --overwrite --out=dist --ignore='^/dist$' --prune --asar --platform=win32 --arch=x64 --version=0.36.5 --icon=assets/epp.ico",
    "clean": "rm -rf node_modules",
    "nodehid-rebuild": "electron-rebuild -v 0.36.5 --pre-gyp-fix --force --module_dir . -w node-hid -e node_modules/electron-prebuilt",
    "postinstall": "electron-rebuild --pre-gyp-fix --force"
  },
  "license": "ISC",
  "dependencies": {
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "imap": "^0.8.16",
    "lodash": "^4.2.1",
    "moment": "^2.11.2",
    "nconf": "^0.8.4",
    "needle": "^1.0.0",
    "node-blink1": "^0.2.2",
    "node-hid": "^0.5.1",
    "object-assign": "^4.0.1",
    "simplecrypt": "^0.1.0",
    "tinycolor2": "^1.3.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-react": "^6.3.13",
    "bootstrap": "^3.3.6",
    "css-loader": "^0.23.1",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.9",
    "electron-rebuild": "^1.1.3",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "node-libs-browser": "^1.0.0",
    "react": "^0.14.7",
    "react-addons-linked-state-mixin": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-color": "^1.3.6",
    "react-contextmenu": "^1.3.0",
    "react-dom": "^0.14.7",
    "react-fontawesome": "^0.3.3",
    "skyweb": "0.0.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
